															<treecell
																label="04 Technical Design">
															</treecell>
														</treerow>
													</treeitem>
													<treeitem
														tooltiptext="${labels.svn.guideline.trunk_documentations_05}">
														<treerow>
															<treecell
																label="05 Testing and QA">
															</treecell>
														</treerow>
													</treeitem>
													<treeitem
														tooltiptext="${labels.svn.guideline.trunk_documentations_06}">
														<treerow>
															<treecell
																label="06 Deployment">
															</treecell>
														</treerow>
													</treeitem>
													<treeitem
														tooltiptext="${labels.svn.guideline.trunk_documentations_07}">
														<treerow>
															<treecell
																label="07 Data Migration">
															</treecell>
														</treerow>
													</treeitem>
												</treechildren>
											</treeitem>
											<treeitem open="false"
												tooltiptext="${labels.svn.guideline.trunk_source_code}">
												<treerow>
													<treecell
														label="Source Code">
													</treecell>
												</treerow>
											</treeitem>
										</treechildren>
									</treeitem>
								</treechildren>
							</treeitem>
						</treechildren>
					</tree>
				</div>
			</div>
